    "nest-winston": "^1.10.2",
    "nestjs-i18n": "^10.5.1",
    "nodemailer": "^7.0.2",
    "passport-jwt": "^4.0.1",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.22",
    "uuid": "^11.1.0",
